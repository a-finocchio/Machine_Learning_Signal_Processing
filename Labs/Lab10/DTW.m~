function D = DTW(S)
    [row,col] = size(S);
    D=zeros(row,col);
    
    % first col
    iter = row-1;
    sum_temp = 0;
    for i_row = 2:row+1      
        sum_temp= sum_temp + abs(S(iter+1,1));
        D(iter+1,1) = sum_temp;
        iter=iter-1;
    end
    
    %last row
    sum_temp = 0;
    for i_row = 2:col+1      
        sum_temp= sum_temp + abs(S(iter+1,1));
        D(iter+1,1) = sum_temp;
        iter=iter-1;
    end


end
